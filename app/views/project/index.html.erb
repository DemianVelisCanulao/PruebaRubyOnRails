
    <h1>Mis proyectos</h1> 
    <%= link_to 'Nuevo Proyecto', new_project_path %>

    <ul>
        <% @projects.each do |project| %>
            <li class="project-container">
            <%= link_to project.title, project %>
            <%= link_to 'Editar', edit_project_path(project) %>
            <%= link_to 'Eliminar', project, data: {  turbo_method: "delete", turbo_confirm: '¿Estás seguro de eliminar el proyecto?'} %>
            </li>   
        <% end %>
    </ul>

